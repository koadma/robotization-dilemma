<body>
	<slider
		id = "objectMainGameTimeSlider"
		minrelx = "0.0"
		minrely = "1.0"
		minabsx = "5.0"
		minabsy = "-25.0"
		maxrelx = "1.0"
		maxrely = "1.0"
		maxabsx = "-5.0"
		maxabsy = "-5.0"
		minvalue = "0"
		maxvalue = "2"
		startvalue = "0"
		quanta = "0.001"
		callback = "ingameTimeSliderInput"
	>Play Story Mode</slider>
	<div
		id = "objectMainGameCanvasContainer"
		minrelx = "0.0"
		minrely = "0.0"
		minabsx = "0.0"
		minabsy = "0.0"
		maxrelx = "0.9"
		maxrely = "1.0"
		maxabsx = "-150.0"
		maxabsy = "-30.0"
	></div>
	<labelbind
		id = "objectMainGameCanvasScale"
		minrelx = "0.9"
		minrely = "0.0"
		minabsx = "-245.0"
		minabsy = "5.0"
		maxrelx = "0.9"
		maxrely = "0.0"
		maxabsx = "-155.0"
		maxabsy = "45.0"
		align = "1"
	>ERROR</labelbind>
	<labelbind
		id = "objectMainGameShipLocation"
		minrelx = "0.0"
		minrely = "0.0"
		minabsx = "5.0"
		minabsy = "45.0"
		maxrelx = "0.0"
		maxrely = "0.0"
		maxabsx = "245.0"
		maxabsy = "75.0"
		align = "0"
	>ERROR</labelbind>
	<labelbind
		id = "objectMainGameShipVelocity"
		minrelx = "0.0"
		minrely = "0.0"
		minabsx = "5.0"
		minabsy = "5.0"
		maxrelx = "0.0"
		maxrely = "0.0"
		maxabsx = "245.0"
		maxabsy = "35.0"
		align = "0"
	>ERROR</labelbind>
	<div
		id = "objectMainGameShipCanvasContainer"
		minrelx = "0.9"
		minrely = "0.0"
		minabsx = "-150.0"
		minabsy = "0.0"
		maxrelx = "1.0"
		maxrely = "0.5"
		maxabsx = "0.0"
		maxabsy = "0.0"
	></div>
	<div
		id = "objectMainGameMenuSidebar"
		minrelx = "0.9"
		minrely = "0.5"
		minabsx = "-150.0"
		minabsy = "0.0"
		maxrelx = "1.0"
		maxrely = "1.0"
		maxabsx = "0.0"
		maxabsy = "-30.0"
	>
		<button
			id = "objectMainGameExitButton"
			minrelx = "0.5"
			minrely = "1.0"
			minabsx = "5.0"
			minabsy = "-45.0"
			maxrelx = "1.0"
			maxrely = "1.0"
			maxabsx = "-5.0"
			maxabsy = "-5.0"
			callback = "ingameMenuExitButton"
		>Exit to menu</button>
		<button
			id = "objectMainGameCommitButton"
			minrelx = "0.0"
			minrely = "1.0"
			minabsx = "5.0"
			minabsy = "-45.0"
			maxrelx = "0.5"
			maxrely = "1.0"
			maxabsx = "-5.0"
			maxabsy = "-5.0"
			callback = "ingameMenuCommitButton"
		>Commit</button>
		<labelbind
			id = "objectSidebarName"
			minrelx = "0.0"
			minrely = "1.0"
			minabsx = "5.0"
			minabsy = "-75.0"
			maxrelx = "1.0"
			maxrely = "1.0"
			maxabsx = "-5.0"
			maxabsy = "-55.0"
			align = "1"
		>ERROR</labelbind>
		<labelbind
			id = "objectSidebarHealth"
			minrelx = "0.0"
			minrely = "1.0"
			minabsx = "5.0"
			minabsy = "-105.0"
			maxrelx = "1.0"
			maxrely = "1.0"
			maxabsx = "-5.0"
			maxabsy = "-85.0"
			align = "1"
		>ERROR</labelbind>
		<button
			id = "objectMainGameEnergyButton"
			minrelx = "0.0"
			minrely = "1.0"
			minabsx = "5.0"
			minabsy = "-135.0"
			maxrelx = "0.5"
			maxrely = "1.0"
			maxabsx = "-5.0"
			maxabsy = "-115.0"
			callback = "ingameMenuEnergyButton"
		>Energy log</button>
		<button
			id = "objectMainGameEnergyButton"
			minrelx = "0.5"
			minrely = "1.0"
			minabsx = "5.0"
			minabsy = "-135.0"
			maxrelx = "1.0"
			maxrely = "1.0"
			maxabsx = "-5.0"
			maxabsy = "-115.0"
			callback = "ingameMenuPowerButton"
		>Power log</button>
		<div
			id = "objectIngameMenuSidebar"
			minrelx = "0.0"
			minrely = "0.0"
			minabsx = "0.0"
			minabsy = "0.0"
			maxrelx = "1.0"
			maxrely = "1.0"
			maxabsx = "0.0"
			maxabsy = "-140.0">
		</div>
	</div>
</body>